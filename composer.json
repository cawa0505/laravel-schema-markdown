{
    "name": "yanzhen0610/laravel-schema-markdown",
    "description": "Make descriptions of schema with markdown for Laravel migrations.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "library",
        "artisan",
        "command",
        "migrations",
        "schema",
        "database",
        "markdown",
        "sqlite",
        "mysql"
    ],
    "authors": [
        {
            "name": "Yz.",
            "email": "yanzhen610@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^5.1",
        "doctrine/dbal": "^2.5.0"
    },
    "autoload": {
        "psr-4": {
            "SchemaMarkdown\\": "src/schema-markdown/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SchemaMarkdown\\MakeSchemaMarkdownProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
